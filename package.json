{
	"name": "flutter-commands",
	"displayName": "Flutter Commands",
	"description": "An extension to simplify flutter commands",
	"publisher": "PratheeshRussell",
	"version": "0.0.2",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/pratheeshrussell/flutter-commands-vscode-ext.git"
	},
	"engines": {
		"vscode": "^1.42.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.runBuildInParts",
		"onCommand:extension.runBuildFat",
		"onCommand:extension.runPackageRepair"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.runBuildInParts",
				"title": "Flutter APK Build:In Parts"
			},
			{
				"command": "extension.runBuildFat",
				"title": "Flutter APK Build:Fat"
			},
			{
				"command": "extension.runPackageRepair",
				"title": "Flutter Repair Import Errors"
			}
		]
		
	},

	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"extensionDependencies": [
		"Dart-Code.dart-code","dart-code.flutter"
	],
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.1",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.42.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.0.1",
		"typescript": "^3.7.5",
		"vscode-test": "^1.3.0"
	},
	"__metadata": {
		"publisherDisplayName": "Pratheesh Russell S"
	}
}
